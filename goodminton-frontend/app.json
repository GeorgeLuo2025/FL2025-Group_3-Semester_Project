{
  "expo": {
    "name": "goodminton",
    "slug": "goodminton",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E5B37"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goodminton.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0E5B37"
      },
      "package": "com.goodminton.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "goodminton",
              "host": "auth",
              "pathPrefix": "/success"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "goodminton",
    "extra": {
      "apiUrl": "http://localhost:3001/api"
    }
  }
}
